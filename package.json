{
    "name": "kicklivebot",
    "version": "1.0.0",
    "description": "Discord bot to send kick notifications",
    "main": "src/index.ts",
    "type": "module",
    "scripts": {
        "build": "esbuild src/index.ts --bundle --minify --platform=node --target=node12 --outdir=dist",
        "start": "node dist/index.js",
        "format": "prettier --write \"src/**/*.ts\"",
        "lint": "tsc --noEmit && eslint **/*.ts *.*"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/LoneCoder21/KickLive.git"
    },
    "keywords": [
        "discord",
        "kick",
        "stream",
        "bot"
    ],
    "author": "Abhinav Neelam <abhinavneelam949@gmail.com> (https://github.com/LoneCoder21)",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/LoneCoder21/KickLive/issues"
    },
    "homepage": "https://github.com/LoneCoder21/KickLive#readme",
    "dependencies": {
        "@types/node": "^20.11.17",
        "discord.js": "^14.14.1",
        "pusher-js": "^8.4.0-rc2"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.0.1",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "esbuild": "0.20.0",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "globals": "^14.0.0",
        "prettier": "3.2.5",
        "typescript": "^5.3.3"
    }
}
