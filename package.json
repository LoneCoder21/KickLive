{
    "name": "kicklivebot",
    "version": "1.0.0",
    "description": "Discord bot to send kick notifications",
    "main": "src/app.ts",
    "type": "module",
    "files": [
        "dist/*"
    ],
    "scripts": {
        "build": "esbuild src/app.ts --bundle --format=esm --platform=node --outdir=dist --external:discord.js --external:pusher-js --external:axios --external:dotenv/config --external:sequelize",
        "prerestart": "npm run build",
        "start": "node dist/app.js",
        "format": "prettier --write \"src/**/*.ts\"",
        "lint": "tsc --noEmit && eslint **/*.ts *.*",
        "build-start": "npm run build && npm run start",
        "dev": "nodemon"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/LoneCoder21/KickLive.git"
    },
    "keywords": [
        "discord",
        "kick",
        "stream",
        "bot"
    ],
    "author": "Abhinav Neelam <abhinavneelam949@gmail.com> (https://github.com/LoneCoder21)",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/LoneCoder21/KickLive/issues"
    },
    "homepage": "https://github.com/LoneCoder21/KickLive#readme",
    "dependencies": {
        "@types/node": "^20.11.17",
        "axios": "^1.6.7",
        "discord.js": "^14.14.1",
        "dotenv": "^16.4.3",
        "pusher-js": "^8.4.0-rc2",
        "reflect-metadata": "^0.2.1",
        "sequelize": "^6.37.0",
        "sequelize-typescript": "^2.1.6",
        "sqlite3": "^5.1.7"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.0.1",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "esbuild": "0.20.0",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "globals": "^14.0.0",
        "nodemon": "^3.0.3",
        "prettier": "3.2.5",
        "typescript": "^5.3.3"
    }
}
